                html: {{ f_html | json_encode() }} [lng_id],
                default: {{ f_default | json_encode() }} [lng_id],
            }
        };

        dialogItems[lng_id] = {
            sortorder: {
                label: {{ text_sortOrder | json_encode() }}[lng_id],
                selector: 'sortOrder',
                state: {{ attributico_sortorder }}
            },
            lazyload: {
